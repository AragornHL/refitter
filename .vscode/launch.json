{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Refitter/bin/Debug/net6.0/refitter.dll",
      "args": [
        "${workspaceFolder}/test/OpenAPI/v3.0/IngramMicro.json",
        "--namespace",
        "Refitter.Test",
        "--output",
        "${workspaceFolder}/test/Refitter.Test.cs"
      ],
      "cwd": "${workspaceFolder}/src/Refitter",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
